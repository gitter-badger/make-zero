{
    "name": "Search Filter",
    "version": "1.0",
    "description": "Filte the search result of Google, Baidu",
    "author": "zhy",
    "permissions": [
        "storage",
        "webRequest",
        "*://www.google.com/",
        "*://www.baidu.com/"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon.png"
    },
    "manifest_version": 2
}